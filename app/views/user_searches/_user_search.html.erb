<div id="user_search">
  <table class="table table-bordered table-hover table-fixed">
    <thead>
      <tr>
        <th scope="col">id</th>
        <th scope="col"><%= t('cars_search.parameters.make')%></th>
        <th scope="col"><%= t('cars_search.parameters.model')%></th>
        <th scope="col"><%= t('cars_search.parameters.year_from')%></th>
        <th scope="col"><%= t('cars_search.parameters.year_to')%></th>
        <th scope="col"><%= t('cars_search.parameters.price_from')%></th>
        <th scope="col"><%= t('cars_search.parameters.price_to')%></th>
      </tr>
    </thead>
    <tbody>
      <% @user_searches.each do |user_search| %>
      <tr>
        <td>
          <%= user_search.id %>
        </td>
        <td>
          <%= user_search.make %>
        </td>
        <td>
          <%= user_search.model %>
        </td>
        <td>
          <%= user_search.year_from %>
        </td>
        <td>
          <%= user_search.year_to %>
        </td>
        <td>
          <%= user_search.price_from%>
        </td>
        <td>
          <%= user_search.price_to %>
        </td>
      </tr>
      <% end %>
    </tbody>
  </table>
</div>
